{
  "version": 2,
  "builds": [
    {
      "src": "main/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      },
      "buildCommand": "main/build_files.sh"  
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main/wsgi.py"
    }
  ],
  "root": "./ezbackend/main"
}
